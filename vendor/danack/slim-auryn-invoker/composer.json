{
    "name": "danack/slim-auryn-invoker",
    "type": "library",
    "description": "Allows route callables to be dispatched by Auryn.",
    "keywords": ["auryn", "di"],
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "rdlowrey/auryn": "^1.4.2"
    },
    "require-dev": {
      "danack/AurynConfig": "^0.9.1",
      "mockery/mockery": "0.9.9",
      "phpunit/phpunit": "6.3.0",
      "pimple/pimple": "^3.0",
      "psr/http-message": "^1.0.1",
      "slim/slim": "3.7.0",
      "squizlabs/php_codesniffer": "^2.8",
      "twig/twig": "^1.33",
      "monolog/monolog": "1.23.0",
      "yoast/yoastcs": "0.4.2",
      "zendframework/zend-diactoros": "^1.5.0"
    },
    "autoload": {
        "psr-0": {
          "Danack\\SlimAurynInvoker\\": "lib/",
          "Danack\\Response\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "DanackTest\\": "test/",
            "Danack\\SlimAurynExample\\": "example/lib/"
        }
    }
}
